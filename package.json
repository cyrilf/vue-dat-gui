{
  "name": "@cyrilf/vue-dat-gui",
  "version": "0.4.0",
  "description": "A Vue2 version of the dat.GUI library",
  "main": "./dist/vue-dat-gui.common.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-lib && npm run copy-demo",
    "build-lib": "vue-cli-service build --name vue-dat-gui --target lib src/main.js",
    "copy-demo": "node ./scripts/copyDemo",
    "lint": "vue-cli-service lint",
    "prepare": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cyrilf/vue-dat-gui.git"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": "~10.2.5"
  },
  "keywords": [
    "vue dat gui",
    "vue",
    "dat.gui"
  ],
  "author": "cyrilf",
  "bugs": {
    "url": "https://github.com/cyrilf/vue-dat-gui/issues"
  },
  "homepage": "https://github.com/cyrilf/vue-dat-gui#readme",
  "dependencies": {
    "core-js": "^3.33.0",
    "lodash.clamp": "^4.0.3",
    "lodash.tonumber": "^4.0.3",
    "v-color": "^1.1.2",
    "vue": "^2.7.15"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.0.0",
    "sass": "^1.61.1",
    "sass-loader": "^13.3.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintIgnore": [
    "dist",
    "public/index.html"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
